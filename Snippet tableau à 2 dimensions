#include <studio.h>
#include <stdlib.h>

int main (int argc, char *argv[])
{
  int ligne; //nbr de ligne
  int colonne; //nbr de colonne
  int tableau [3][4]; 
  
  tableau[0][0] = 1;
  tableau[0][1] = 2;
  tableau[0][2] = 3;
  tableau[0][3] = 4;
  tableau[1][0] = 5;
  tableau[1][1] = 6;
  tableau[1][2] = 7;
  tableau[1][3] = 8;
  tableau[2][0] = 9;
  tableau[2][1] = 10;
  tableau[2][2] = 11;
  tableau[2][3] = 12;
  
  for (ligne = 0; ligne < 3; ligne++)
  {
    for (colonne = 0; colonne < 4; colonne++)
    {
      printf ("%d\n", tableau [ligne][colonne]);
    }
  }
  
  return 0;
}
  




